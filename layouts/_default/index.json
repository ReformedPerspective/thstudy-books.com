{{- $.Scratch.Add "index" slice -}}
{{- range .Site.RegularPages -}}
{{- $.Scratch.Add "index" (dict "title" .Title "categories" .Params.categories "contents" .Plain "permalink" .Permalink "subtitle" .Params.subtitle "description" .Params.description "publisher" .Params.publisher "author" .Params.author) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}